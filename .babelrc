{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": "> 0.25%, not dead"
      }
    ],
    [
      "@babel/preset-typescript", {
        "isTSX": true,
        "jsxPragma": "React",
        "allExtensions": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    // Required for Typescript
    "@babel/proposal-class-properties",
    "transform-es2015-modules-commonjs",

    // Others
    "@babel/plugin-proposal-numeric-separator",
  ]
}
