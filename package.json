{
  "name": "gatsby-typescript-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby Typescript",
  "version": "1.0.0",
  "author": "Le Anh Duc <anhdle14@hotmail.com>",
  "dependencies": {
    "gatsby": "^2.15.12",
    "gatsby-image": "^2.2.18",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sharp": "^2.2.21",
    "gatsby-plugin-typescript": "^2.1.7",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-sharp": "^2.2.14",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.6.2"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "node_modules/.bin/babel-node --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby develop",
    "build": "node_modules/.bin/babel-node --presets @babel/preset-typescript --extensions '.ts,.tsx' node_modules/.bin/gatsby build",
    "clean": "rm -rf .cache public",
    "type-check": "tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anhdle14/gatsby-typescript-starter-default"
  },
  "bugs": {
    "url": "https://github.com/anhdle14/gatsby-typescript-starter-default/issues"
  }
}
